.member_info{
  background: white;
  margin:20px 10px;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding:30px;
  box-shadow: 2px 2px 6px #C0C0C0;
}
.member_info > view{
  width:70px;
  height:70px;
  background: #F5F5F5;
  margin:10px;

}
.member_info > view>image.plus{
  width:50%;
  height:50%;
  position: relative;
  left:25%;
  top:25%;
}
.button_group{
  box-shadow: 2px 2px 6px #C0C0C0;
  margin:20px 10px;
  text-align: center;
}
view.start{
  width:100%;
  padding: 20px 0;
  color:#2d405f;
}
