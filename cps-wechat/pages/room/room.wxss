page{
  height:100%;
  box-sizing: border-box;
}
.room_info{
  height:140px;
  box-shadow: 2px 0 6px #C0C0C0;
  color:white; 
  line-height: 140px;
  text-align: center;
  font-size: 30px;
}
.room_info>image{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:140px;
  z-index: -99;
}
.iconfont{
  font-family: "iconfont" !important;
  color: white;
}
